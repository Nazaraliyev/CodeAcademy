@{
	ViewData["Title"] = "Create";
}

@model IdentityRole


<div class="container-fluid">
	<h2 class = "mb-5">Create New Roll</h2>
	<form class="w-50" asp-area = "admin" asp-controller = "Rolls" asp-action = "Create" method = "post">
		<div class="mb-3">
			<label asp-for = "@Model.Name" class="form-label">Roll Name</label>
			<input class="form-control" asp-for = "@Model.Name">
		</div>
		<button type = "submit" class = "btn btn-primary">Create</button>
	</form>
</div>


