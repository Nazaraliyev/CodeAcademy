@model IdentityRole


<div class="container-fluid">
	<h2 class = "mb-5">Create New Roll</h2>
	<form class = "w-50" asp-area="admin" asp-controller="Roll" asp-action="Update" method="post">
		<div class="mb-3">
			<label asp-for="@Model.Name" class="form-label">Roll Name</label>
			<input asp-for="@Model.Name" class="form-control" />
		</div>
		<input asp-for = "@Model.Id" hidden />
		<input asp-for = "@Model.ConcurrencyStamp" hidden />
		<button type="submit" class="btn btn-primary">Update</button>
	</form>
</div>
